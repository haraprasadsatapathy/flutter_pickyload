# Android Local Properties Template
# Copy this file to local.properties and fill in your actual values
# DO NOT commit local.properties to version control

# Android SDK location (update with your path)
sdk.dir=/path/to/Android/Sdk

# Flutter SDK location (update with your path)
flutter.sdk=/path/to/flutter

# Build configuration
flutter.buildMode=debug
flutter.versionName=1.0.0
flutter.versionCode=1

# Google Maps API Key (keep this secret, never commit to version control)
# Get your API key from: https://console.cloud.google.com/google/maps-apis
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
